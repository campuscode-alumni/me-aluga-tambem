<%= simple_form_for([@property,@proposal]) do |f| %>
  <div class="form-group">
    <%= f.input :start_date, label: 'Data de entrada', html5: true %>
    <%= f.input :end_date, label: 'Data de saída',html5: true %>
    <%= f.input :rent_proposal, label: 'Motivo' %>
    <%= f.input :total_guests, label: 'Quantidade de hóspedes' %>

  </div>

  <%= f.submit 'Efetuar Proposta', class: 'btn btn-primary' %>
<% end %>
